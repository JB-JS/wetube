extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching by #{searchingBy}
        if videos.length === 0
            h3 Video is Not Found
        each item in videos
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videofile: item.fileUrl
            })